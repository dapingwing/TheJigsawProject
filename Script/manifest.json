{
	"manifest_version": 2,
	"name": "PkaJigsaw",
	"version": "0.1.0.0",
	"description": "Pka Jigsaw system",
	"permissions": [
    	"tabs","<all_urls>"
    ],
  	"browser_action": {
		"default_icon": "icon.gif",
		"default_popup": "popup.html",
		"default_title": "PKA TOOLS"
  },
  "content_scripts": [
        {
        "matches": [
            "http://pegasus.astroempires.com/*",
            "https://pegasus.astroempires.com/*"
            ],
		"css": ["jquery-ui-1.10.4.css"],
        "js": ["jquery.js","jquery-ui-1.10.4.js","prefixfree-1.0.7.js","aescripts/js_bbcode_extras_v1.1.js","aescripts/js_move_v1.4.js","aescripts/js_timers_v1.11.js","aescripts/js_jquery_fleet_move_here_v1.0.js","aescripts/vector_graphics.js","functions.js","content.js"],
        "run_at": "document_end"
        }
    ],
 	"web_accessible_resources": [
		"Interfaces/*",
		"Pieces/*"
	]
}